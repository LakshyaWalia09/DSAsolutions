class Solution
{
public:
    int findCatalan(int n)
    {
        vector<int> cn(n + 1);
        cn[0] = cn[1] = 1;
        for (int i = 2; i <= n; i++)
        {
            for (int j = 0; j <= n - 1; j++)
            {
                cn[i] += cn[j] * cn[i - 1 - j];
            }
        }
        return cn[n];
    }
};
